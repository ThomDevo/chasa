<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd"
             version="2.2">
    <persistence-unit name="chasa" transaction-type="JTA">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <jta-data-source>java:comp/env/jdbc/chasa</jta-data-source>
        <class>com.example.chasa.entities.AddressesEntity</class>
        <class>com.example.chasa.entities.CharacteristicsEntity</class>
        <class>com.example.chasa.entities.CitiesEntity</class>
        <class>com.example.chasa.entities.CountriesEntity</class>
        <class>com.example.chasa.entities.DiveSiteCharacteristicsEntity</class>
        <class>com.example.chasa.entities.DiveSitesEntity</class>
        <class>com.example.chasa.entities.EventCategoriesEntity</class>
        <class>com.example.chasa.entities.EventsEntity</class>
        <class>com.example.chasa.entities.HyperbaricchambersEntity</class>
        <class>com.example.chasa.entities.LicenseUsersEntity</class>
        <class>com.example.chasa.entities.LicensesEntity</class>
        <class>com.example.chasa.entities.MedicalCertificatesEntity</class>
        <class>com.example.chasa.entities.PermissionsEntity</class>
        <class>com.example.chasa.entities.RolePermissionEntity</class>
        <class>com.example.chasa.entities.RolesEntity</class>
        <class>com.example.chasa.entities.UserEventsEntity</class>
        <class>com.example.chasa.entities.UsersEntity</class>
    </persistence-unit>
</persistence>
